{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "lint": {},
        "build": {
            "dependsOn": [
                "lint"
            ],
            "outputs": [
                "build/**"
            ]
        },
        "@frontend-mentor/news-homepage#build": {
            "cache": false,
            "dependsOn": [
                "lint"
            ],
            "outputs": [
                ".next/**",
                "out/**"
            ]
        },
        "copy": {
            "cache": false,
            "dependsOn": [
                "build"
            ]
        }
    }
}